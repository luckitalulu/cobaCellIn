    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="28" />

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.myapplication.AppController"
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@drawable/cellin"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cellin"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.myapplication.HalUtamaChoice" >
        </activity>
        <activity android:name="com.example.myapplication.HalUtama3G" >
        </activity>
        <activity android:name="com.example.myapplication.HalUtama2G" >
        </activity>
        <activity android:name="com.example.myapplication.Register" >
        </activity>
        <!-- <activity android:name=".Util.Server"> -->
        <!-- </activity> -->
        <!-- <activity android:name=".AppController"> -->
        <!-- </activity> -->
        <activity android:name="com.example.myapplication.HalUtama4G" >
        </activity>
        <activity android:name="com.example.myapplication.LogIn" >
        </activity>
        <activity android:name="com.example.myapplication.splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.example.myapplication.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>